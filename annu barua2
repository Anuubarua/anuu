#include <stdio.h>

int main() {
    int array[5] = {10, 20, 30, 40, 50};
    
    // Printing addresses of elements in the array
    printf("Addresses of elements in the array:\n");
    for (int i = 0; i < 5; ++i) {
        printf("Address of array[%d]: %p\n", i, (void *)&array[i]);
    }
    
    return 0;
}
